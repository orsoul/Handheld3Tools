    <item>烟台_535</item>
    <item>潍坊_536</item>
    <item>成都_028</item>
  </string-array>
  <string-array name="array_name_can">
    <item>完整</item>
    <item>残损</item>
  </string-array>
  <string-array name="array_name_dai">
    <item>100元</item>
